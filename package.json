{
  "name": "docusaurus-mdx-checker-diff",
  "author": "beveradb",
  "license": "MIT",
  "version": "1.0.0",
  "description": "Check modified files for Docusaurus content for MDX v2 parsing errors",
  "homepage": "https://github.com/beveradb/docusaurus-mdx-checker-diff#readme",
  "keywords": [
    "mdx",
    "docusaurus",
    "markdown",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beveradb/docusaurus-mdx-checker-diff.git"
  },
  "bugs": {
    "url": "https://github.com/beveradb/docusaurus-mdx-checker-diff/issues"
  },
  "main": "src/main.js",
  "bin": "src/cli.js",
  "type": "module",
  "scripts": {
    "launch:docusaurus-v2": "node src/cli.js --cwd ./__tests__/__fixtures__/sites/docusaurus-v2",
    "launch:docusaurus-v3": "node src/cli.js --cwd ./__tests__/__fixtures__/sites/docusaurus-v3",
    "test": "vitest --test-timeout 15000"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.0.0",
    "@slorber/remark-comment": "^1.0.0",
    "acorn": "^8.10.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "globby": "^13.2.2",
    "lodash": "^4.17.21",
    "periscopic": "^3.1.0",
    "remark-directive": "^3.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "prettier": "^3.0.3",
    "vitest": "^0.34.3"
  },
  "packageManager": "yarn@4.5.0"
}
